<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lemon.print.storeInfo.mapper.StoreInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lemon.print.storeInfo.entity.StoreInfo">
        <id column="unitguid" property="unitguid" />
        <result column="store_name" property="storeName" />
        <result column="store_address" property="storeAddress" />
        <result column="is_delete" property="isDelete" />
        <result column="create_time" property="createTime" />
        <result column="create_user" property="createUser" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        unitguid, store_name, store_address, is_delete, create_time, create_user
    </sql>

</mapper>
